{
    "log": {
	"dual": { "mode": "log", "mirror": "fc00::1" },
	"mirror": { "mirror": [ "10.0.0.1", "10.0.0.2", "fc00::2" ] },
	"nflog": { "mode": "nflog", "group": 1, "range": 128 },
	"none": { "mode": "none" },
	"ulog": { "mode": "ulog", "limit": { "interval": 5 } },
	"zero": { "mode": "log", "limit": 0 }
    },
    "packet-log": [
	{ "out": "_fw" },
	{ "out": "_fw", "log": "mirror" },
	{ "out": "_fw", "log": "nflog" },
	{ "out": "_fw", "log": "ulog" }
    ],
    "filter": { "in": "_fw", "log": "ulog", "action": "pass" }
}
